{
  "name": "express-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cssStart": "npm-run-all --parallel dev watch:sass",
    "compress:sass": "node-sass --output-style compressed -o dist/css sass/",
    "compile:sass": "node-sass sass/main.scss dist/css/style.comp.css",
    "concat:css": "concat -o dist/css/style.concat.css dist/css/style.comp.css",
    "prefix:css": "postcss -u autoprefixer -r dist/css/*",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:sass",
    "start": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "autoprefixer": "^9.1.0",
    "concat": "^1.0.3",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}